    "core-js": "^3.36.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-cleanup": "^3.2.1",
